<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.betteridgeh16.revisionapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/launcher_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/NewAppTheme">
        <activity
            android:name=".Activities.Home"
            android:label="@string/app_name"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.Launcher">
            "
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.CoursesList"
             />
        <activity
            android:name=".Activities.ExamBoards"
             />
        <activity
            android:name=".Activities.Course"
            android:label="@string/title_activity_course"
             />
        <activity
            android:name=".Activities.Revise"
            android:label="@string/title_activity_revise"
            ></activity>
    </application>

</manifest>